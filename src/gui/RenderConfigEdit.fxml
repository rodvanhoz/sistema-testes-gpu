<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="105.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PlacaDeVideoCaracsController">
   <children>
      <Label text="Editar Render Config">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="33.0" prefWidth="375.0">
         <children>
            <TextField fx:id="txtShadingUnits" prefHeight="25.0" prefWidth="94.0" promptText="Shading Units">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtTMUs" prefHeight="25.0" prefWidth="86.0" promptText="TMUs">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtROPs" prefHeight="25.0" prefWidth="80.0" promptText="ROPs">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtSmCount" prefHeight="25.0" prefWidth="80.0" promptText="SM Count">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="36.0" prefWidth="375.0">
         <children>
            <TextField fx:id="txtL1Cache" prefHeight="25.0" prefWidth="94.0" promptText="L1 Cache">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtL2Cache" prefHeight="25.0" prefWidth="85.0" promptText="L2 Cache">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtTensorCores" prefHeight="25.0" prefWidth="80.0" promptText="Tensor Cores">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtRtCores" prefHeight="25.0" prefWidth="80.0" promptText="RT Cores">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="btSalvarRenderConfig" mnemonicParsing="false" onAction="#onBtSalvarRenderConfigAction" text="Salvar" />
            <Button fx:id="btCancelarRenderConfig" mnemonicParsing="false" onAction="#onBtCancelarRenderConfigAction" text="Cancelar" />
         </buttons>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
