<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="113.0" prefWidth="782.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PlacaDeVideoCaracsController">
   <children>
      <Label text="Editar Processador Gráfico">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox prefHeight="34.0" prefWidth="782.0">
         <children>
            <TextField fx:id="txtNomeGpu" prefHeight="25.0" prefWidth="248.0" promptText="Nome GPU">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtVariantGpu" prefHeight="25.0" prefWidth="257.0" promptText="Variant GPU">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtArquitetura" prefHeight="25.0" prefWidth="247.0" promptText="Arquitetura">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="34.0" prefWidth="782.0">
         <children>
            <TextField fx:id="txtFundicao" prefHeight="25.0" prefWidth="320.0" promptText="Fundição">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtLitografia" prefHeight="25.0" prefWidth="108.0" promptText="Litografia (nm)">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtNroTransistores" prefHeight="25.0" prefWidth="158.0" promptText="Nº Transistores">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="txtTamanhoChip" prefHeight="25.0" prefWidth="158.0" promptText="Tamanho do Chip (mm) ">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="btSalvarProcessadorGrafico" mnemonicParsing="false" onAction="#onBtSalvarProcessadorGraficoAction" text="Salvar" />
            <Button fx:id="btCancelarProcessadorGrafico" mnemonicParsing="false" onAction="#onBtCancelarProcessadorGraficoAction" text="Cancelar" />
         </buttons>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
