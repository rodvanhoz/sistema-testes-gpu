<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="125.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ConfiguracoesJogosController">
   <children>
      <Label text="Editar Relação Configurações / Jogos">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ComboBox fx:id="cboxJogos" editable="true" onKeyReleased="#onCBoxJogosKeyPressedAction" prefHeight="25.0" prefWidth="375.0" promptText="Jogo">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </ComboBox>
      <ComboBox fx:id="cboxConfiguracoes" editable="true" onKeyReleased="#onCBoxConfiguracoesKeyPressesAction" prefHeight="25.0" prefWidth="375.0" promptText="Configuração">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </ComboBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="btSalvar" mnemonicParsing="false" onAction="#onBtSalvarAction" text="Salvar" />
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#onBtCancelarAction" text="Cancelar" />
        </buttons>
         <VBox.margin>
            <Insets right="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="5.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
